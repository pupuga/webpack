{
  "name": "pupugapack",
  "version": "4.0.0",
  "description": "webpack config",
  "main": "webpack.config.js",
  "author": "Alex Shandor",
  "license": "MIT",
  "scripts": {
    "docker_build": "NODE_PATH=../../node_modules yarn install --modules-folder ../../node_modules && NODE_PATH=../../node_modules ../../node_modules/.bin/webpack --mode production",
    "docker_dev": "NODE_PATH=../../node_modules yarn install --modules-folder ../../node_modules && NODE_PATH=../../node_modules ../../node_modules/.bin/webpack --mode development --watch",
    "docker_start": "NODE_PATH=../../node_modules yarn install --modules-folder ../../node_modules && NODE_PATH=../../node_modules ../../node_modules/.bin/webpack serve --mode development",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "start": "webpack serve --mode development"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "autoprefixer": "latest",
    "babel-loader": "latest",
    "clean-webpack-plugin": "latest",
    "copy-webpack-plugin": "latest",
    "css-loader": "latest",
    "cssnano": "latest",
    "html-webpack-plugin": "latest",
    "mini-css-extract-plugin": "latest",
    "postcss": "latest",
    "postcss-loader": "latest",
    "sass": "latest",
    "sass-loader": "latest",
    "style-loader": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "webpack-dev-server": "latest",
    "webpack-manifest-plugin": "latest"
  }
}
